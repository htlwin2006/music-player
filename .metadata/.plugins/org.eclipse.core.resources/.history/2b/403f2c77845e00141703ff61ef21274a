package com.mysquar.mymusicplayer;

public class MusicTrack {
	private int mp3URI;
	private int thumbnailURI;
	
	/**
	 * 
	 * @param mp3URI uri of mp3
	 * @param thumbnailURI
	 */
	public MusicTrack(int mp3URI,int thumbnailURI) {
		this.mp3URI = mp3URI;
		this.thumbnailURI = thumbnailURI;
	}
	
	public int getMp3URI() {
		return mp3URI;
	}
	
	public int getThumbnailURI() {
		return thumbnailURI;
	}
	
	public void setMp3URI(String mp3uri) {
		mp3URI = mp3uri;
	}
	
	public void setThumbnailURI(String thumbnailURI) {
		this.thumbnailURI = thumbnailURI;
	}
	
}
